#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main() {
	float x, y;
	
printf("Entre com o valor de X: ");
	scanf("%f", &x);
printf("Entre com o valor de y: ");
	scanf("%f", &y);
	
if ( x > 0 && y > 0){
printf("No Quadrante 1");
	} else if (x > 0 && y < 0){
printf("No Quadrante 4");
	} else if (x < 0 && y < 0){
printf("No Quadrante 3");
	} else if (x < 0 && y > 0){
printf("No Quadrante 2");
	} else if (x == 0 && y == 0){
printf("Origem");
	}
}
